    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.2",
    "@types/node-fetch": "^2.5.4",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "chromedriver": "^79.0.0",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "expect": "^24.9.0",
    "geckodriver": "^1.19.1",
