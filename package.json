{
  "name": "nightwatch-api",
  "version": "0.4.0",
  "main": "./lib/index.js",
  "types": "./lib/main.d.ts",
  "engines": {
    "node": ">= 8.0.0"
  },
  "repository": "git@github.com:mucsi96/nightwatch-api.git",
  "author": "Igor Mucsicska <mucsi_96@yahoo.com>",
  "license": "MIT",
  "scripts": {
    "clean": "del-cli lib",
    "build": "tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts,.tsx\"",
    "type-check": "tsc --noEmit",
    "start": "cross-env DEBUG=nightwatch-api babel-node --extensions \".ts,.tsx\" test/server.ts",
    "test": "cross-env DEBUG=nightwatch-api jest --no-cache",
    "coverage": "jest --coverage",
    "prepublishOnly": "yarn clean && yarn build",
    "debug":
      "cross-env DEBUG=nightwatch-api babel-node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "ci": "start-server-and-test http://localhost:3000",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "precommit": "pretty-quick --staged",
    "build:doc": "typedoc --json typedoc.json --mode modules src/index.ts",
    "build:site": "del-cli site-dist && webpack --config ./site/webpack.config.js",
    "start:site": "webpack-serve --config ./site/webpack.config.js"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "@types/debug": "^0.0.30",
    "nightwatch": "^1.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.4",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-styled-components": "^1.5.1",
    "chai": "^4.1.2",
    "chromedriver": "2.38.3",
    "cross-env": "^5.1.5",
    "del-cli": "^1.1.0",
    "geckodriver": "1.11.0",
    "highlight.js": "^9.12.0",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lodash.omit": "^4.5.0",
    "prettier": "1.12.1",
    "pretty-quick": "^1.5.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-markdown": "^3.3.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-svg-loader": "^2.1.0",
    "react-syntax-highlighter": "^7.0.4",
    "selenium-server": "3.12.0",
    "simple-icons": "^1.6.4",
    "slug": "^0.9.1",
    "start-server-and-test": "^1.4.1",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "styled-components": "^3.3.3",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "twemoji": "^11.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "wait-on": "^2.1.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-serve": "^1.0.4"
  }
}
