{
  "name": "nightwatch-api",
  "version": "0.4.0",
  "main": "./lib/index.js",
  "files": [
    "lib",
    "types"
  ],
  "types": "./lib/main.d.ts",
  "engines": {
    "node": ">= 8.0.0"
  },
  "repository": "git@github.com:mucsi96/nightwatch-api.git",
  "author": "Igor Mucsicska <mucsi_96@yahoo.com>",
  "license": "MIT",
  "scripts": {
    "add-contributor": "all-contributors add",
    "clean": "del-cli lib",
    "build": "tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\"",
    "type-check": "tsc --noEmit",
    "start": "cross-env DEBUG=nightwatch-api babel-node --extensions \".ts\" test/server.ts",
    "test": "cross-env DEBUG=nightwatch-api jest --no-cache",
    "test:coverage": "npm test -- --coverage",
    "test:coveralls": "cat ./coverage/lcov.info | coveralls",
    "test:ci": "start-server-and-test start http://localhost:3000 test:coverage",
    "prepack": "npm run clean && npm run build",
    "pack-into-examples": "node test/pack-into-examples.js",
    "debug": "cross-env DEBUG=nightwatch-api NIGHTWATCH_VERBOSE=true babel-node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "build:doc": "typedoc --json typedoc.json --mode modules src/index.ts",
    "build:site": "del-cli site-dist && webpack --config ./site/webpack.config.js && del-cli site-dist/site-server-renderer.js",
    "start:site": "webpack-serve --config ./site/webpack.config.js"
  },
  "dependencies": {
    "debug": "^4.0.1",
    "wait-on": "^3.1.0",
    "@types/debug": "^0.0.31"
  },
  "peerDependencies": {
    "nightwatch": "^1.0.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@sindresorhus/slugify": "^0.3.0",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "all-contributors-cli": "^5.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "babel-plugin-styled-components": "^1.8.0",
    "chai": "^4.2.0",
    "chromedriver": "2.42.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "del-cli": "^1.1.0",
    "file-loader": "^2.0.0",
    "geckodriver": "1.12.2",
    "highlight.js": "^9.12.0",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "lodash.omit": "^4.5.0",
    "nightwatch": "^1.0.11",
    "prettier": "1.14.3",
    "pretty-quick": "^1.7.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-markdown": "^4.0.2",
    "react-router": "^4.3.1",
    "react-syntax-highlighter": "^8.0.1",
    "selenium-server": "3.14.0",
    "simple-icons": "^1.9.5",
    "start-server-and-test": "1.5.0",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "styled-components": "^3.4.9",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "twemoji": "^11.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-node-externals": "^1.7.2",
    "webpack-serve": "^2.0.2"
  }
}
